<% case @chirurgie.nom %>
  <% when 'Cataracte' %>
    <%= render "cataracte" %>
  <% when 'Glaucome' %>
    <%= render "glaucomechir" %>
  <% when 'IVT' %>
    <%= render "ivt" %>
  <% when 'Chirurgie réfractive' %>
    <%= render "laser" %>
  <% when 'Chirurgie des paupières' %>
    <%= render "paupieres" %>
<% end %>
