<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "MedicalProcedure",
  "url": "<%= pathology_url(@pathologie) %>",
  "headline": "<%= @pathologie.nom %>"
}
</script>

<% case @pathologie.nom %>
  <% when 'Glaucome' %>
    <%= render "glaucomepatho" %>
  <% when 'Diabète' %>
    <%= render "diabete" %>
  <% when 'DMLA' %>
    <%= render "dmla" %>
  <% when 'Décolement de la rétine' %>
    <%= render "decollement" %>
<% end %>
